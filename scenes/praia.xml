<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="0" />
  </INITIALS>


  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
  </ILLUMINATION>


  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light0">
      <enable value ="1" />
      <position  x="0" y="10" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light1">
      <enable value ="1" />
      <position x="30" y="10" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light2">
      <enable value ="1" />
      <position x="0" y="20" z="20" w="1" />
      <ambient r="0.15" g="0.15" b="0.15" a="1" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
      <enable value ="1" />
      <position x="30" y="10" z="20" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

  </LIGHTS>

  <!-- Textures -->
  <TEXTURES>
    <!-- Tree trunk -->
    <TEXTURE id="trunk">
      <file path="praiaimg/trunk-image.jpg" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

    <TEXTURE id="bed_orange_sheet">
      <file path="praiaimg/orange_sheet.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="bed_orange_sheet_shade">
      <file path="praiaimg/orange_sheet.png" />
      <amplif_factor s="11" t="1" />
    </TEXTURE>

    <!-- beach ball -->
    <TEXTURE id="beach_ball">
      <file path="praiaimg/ball.png" />
      <amplif_factor s="0.5" t="0.5" />
    </TEXTURE>

    <TEXTURE id="sand">
      <file path="praiaimg/sand.png" />
      <amplif_factor s="4" t="4" />
    </TEXTURE>

    <TEXTURE id="water">
      <file path="praiaimg/water.jpg" />
      <amplif_factor s="4" t="4" />
    </TEXTURE>

    <TEXTURE id="rust_submarine">
      <file path="praiaimg/rust.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="metal_submarine">
      <file path="praiaimg/metal.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="glass_eye_submarine">
      <file path="praiaimg/glass-eye.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="sub_metal_submarine">
      <file path="praiaimg/sub-metal.png" />
      <amplif_factor s="3" t="3" />
    </TEXTURE>

    <TEXTURE id="sub_metal_submarine_fronts">
      <file path="praiaimg/sub-metal.png" />
      <amplif_factor s="0.3" t="0.8" />
    </TEXTURE>

    <TEXTURE id="surface_sun">
      <file path="praiaimg/surface_sun.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="surface_blue_sun">
      <file path="praiaimg/surface_blue_sun.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="sand_to_sea">
      <file path="praiaimg/beach_to_sea.jpg" />
      <amplif_factor s="20" t="4" />
    </TEXTURE>

    <TEXTURE id="red_stripes_straw">
      <file path="praiaimg/straw.png" />
      <amplif_factor s="2" t="2" />
    </TEXTURE>

  </TEXTURES>


  <MATERIALS>

    <!-- Blue tennis racket material -->
    <MATERIAL id="blue_tennis_racket">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="0" />
      <diffuse  r="0" g="0.2" b="1" a="1" />
      <ambient r="0" g="0.2" b="1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Red tennis racket material -->
    <MATERIAL id="red_tennis_racket">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="0" />
      <diffuse  r="1" g="0" b="0" a="1" />
      <ambient r="1" g="0" b="0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Cup liquid - orange material -->
    <MATERIAL id="orange_juice">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="0" />
      <diffuse  r="1" g="0.57" b="0" a="1" />
      <ambient r="1" g="0" b="0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Submarine exterior material -->
    <MATERIAL id="sunMaterial">
      <shininess value="10" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="1.0" g="1.0" b="1.0" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

  </MATERIALS>


  <!-- The leaves -->
  <LEAVES>
  </LEAVES>


  <!-- The nodes -->
  <NODES>

    <ROOT id="root" />

    <!-- The root node -->

    <NODE id="root">
      <MATERIAL id="defaultMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>

        <NODEREF id="sea" />
        <NODEREF id="sand_ramp" />
        <NODEREF id="sand" />
        <NODEREF id="beds" />
        <NODEREF id="sunshade" />
        <NODEREF id="beach_ball_sphere"/>
        <NODEREF id="tennis_rackets"/>
        <NODEREF id="sun"/>
        <NODEREF id="blue_sun"/>
        <NODEREF id="submarine"/>
        <NODEREF id="table"/>
        <NODEREF id="wave_water"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="wave_water">
      <MATERIAL id="null" />
      <TEXTURE id="water" />

      <TRANSLATION x="0" y="2.5" z="0" />
      <DESCENDANTS>
        <LEAF id="wave" type="patch" args="30 30" >
          <CPLINE>
            <CPOINT xx="-5.2" yy="-2.5" zz="0" ww="1"/>
            <CPOINT xx="2.6" yy="1.8" zz="0" ww="1"/>
            <CPOINT xx="-5.9" yy="-3.6" zz="0" ww="0.5"/>
            <CPOINT xx="1.6" yy="-2.5" zz="0" ww="0.3"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="-5.2" yy="-2.5" zz="20" ww="1"/>
            <CPOINT xx="2.6" yy="1.8" zz="20" ww="1"/>
            <CPOINT xx="-5.9" yy="-3.6" zz="20" ww="0.5"/>
            <CPOINT xx="1.6" yy="-2.5" zz="20" ww="0.3"/>
          </CPLINE>
        </LEAF>
      </DESCENDANTS>
    </NODE>

    <!--:::::::::::::::::::::::SUBMARINE  :::::::-->

    <NODE id="submarine">
      <MATERIAL id="null" />
      <!-- <TEXTURE id="rust_submarine" /> -->
      <TEXTURE id="sub_metal_submarine"/>

      <TRANSLATION x="2.7" y="-0.5" z="8.3" />
      <ROTATION axis="z" angle="30" />
      <ROTATION axis="y" angle="90" />


      <DESCENDANTS>
        <NODEREF id="front_of_submarine"/>
        <NODEREF id="back_of_submarine"/>
        <NODEREF id="body_of_submarine"/>
        <NODEREF id="periscope_submarine"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="periscope_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <TRANSLATION x="1" y="0" z="0" />
      <DESCENDANTS>
        <NODEREF id="periscope_vertical_of_submarine"/>
        <NODEREF id="periscope_horizontal_of_submarine"/>
        <NODEREF id="periscope_articulate_point_submarine"/>
        <NODEREF id="periscope_lookout_submarine"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="periscope_lookout_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="glass_eye_submarine" />

      <TRANSLATION x="1.5" y="3.5" z="0" />
      <ROTATION axis="z" angle="90" />

      <DESCENDANTS>
        <LEAF id="periscope_lookout_submarine_sphere" type="sphere" args="0.3 2 40" />
      </DESCENDANTS>

    </NODE>

    <NODE id="periscope_articulate_point_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="y" angle="90" />
      <TRANSLATION x="0" y="3.5" z="0" />

      <DESCENDANTS>
        <LEAF id="front_of_submarine_sphere" type="sphere" args="0.3 40 20" />
      </DESCENDANTS>

    </NODE>

    <NODE id="periscope_horizontal_of_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="y" angle="90" />
      <TRANSLATION x="0" y="3.5" z="0" />
      <DESCENDANTS>
        <LEAF id="periscope_horizontal_of_submarine_cylinder" type="cylinder" args="1.5 0.3 0.3 20 40 0 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="periscope_vertical_of_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="90" />
      <TRANSLATION x="0" y="0" z="-3.5" />

      <DESCENDANTS>
        <LEAF id="periscope_vertical_of_submarine_cylinder" type="cylinder" args="2 0.3 0.3 20 40 0 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="body_of_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="null" />


      <ROTATION axis="y" angle="90" />
      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="0" y="0" z="-5.1" />
      <DESCENDANTS>
        <LEAF id="body_of_submarine_cylinder" type="cylinder" args="10.30 1.50 1.50 20 40 0 0" />
      </DESCENDANTS>

    </NODE>

    <NODE id="front_of_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="sub_metal_submarine_fronts" />

      <TRANSLATION x="5" y="0" z="0" />
      <SCALE sx="1.5" sy="1" sz="1" />

      <DESCENDANTS>
        <LEAF id="front_of_submarine_sphere" type="sphere" args="1.5 40 20" />
      </DESCENDANTS>

    </NODE>

    <NODE id="back_of_submarine">
      <MATERIAL id="null" />
      <TEXTURE id="sub_metal_submarine_fronts" />

      <TRANSLATION x="-5" y="0" z="0" />
      <SCALE sx="1.5" sy="1" sz="1" />

      <DESCENDANTS>
        <LEAF id="back_of_submarine_sphere" type="sphere" args="1.5 40 20" />
      </DESCENDANTS>

    </NODE>

    <!--::::::::::::::::::::::::SUN AND BEACH BALL::::::::::::::-->
    <!--:::::::::::::::::::::::SMALL OBJS :::::::-->

    <NODE id="beach_ball_sphere">
      <MATERIAL id="null" />
      <TEXTURE id="beach_ball" />

      <TRANSLATION x="15" y="0.85" z="12.5" />
      <SCALE sx="1" sy="1" sz="1" />

      <DESCENDANTS>
        <LEAF id="sphere_ball" type="sphere" args="0.25 80 20" />
      </DESCENDANTS>

    </NODE>

    <NODE id="blue_sun">
      <MATERIAL id="sunMaterial" />
      <TEXTURE id="surface_blue_sun" />

      <TRANSLATION x="-5" y="15" z="22" />
      <SCALE sx="1" sy="1" sz="1" />
      <ROTATION axis="y" angle="100"/>

      <DESCENDANTS>
        <LEAF id="sun_blue" type="sphere" args="0.8 80 20" />
      </DESCENDANTS>

    </NODE>

    <NODE id="sun">
      <MATERIAL id="sunMaterial" />
      <TEXTURE id="surface_sun" />

      <TRANSLATION x="0" y="20" z="20" />
      <SCALE sx="1" sy="1" sz="1" />

      <DESCENDANTS>
        <LEAF id="sun" type="sphere" args="1.0 80 20" />
      </DESCENDANTS>

    </NODE>

    <!--::::::::::::::::::::::: BEDS      :::::::-->

    <NODE id="beds">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <!-- <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="20" y="1.2" z="10" />
      <ROTATION axis="y" angle="25" /> -->

      <DESCENDANTS>
        <NODEREF id="bed1" />
        <NODEREF id="bed2" />
      </DESCENDANTS>

    </NODE>

    <NODE id="bed1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="20" y="1.19" z="7.5" />
      <ROTATION axis="y" angle="-15" />

      <DESCENDANTS>
        <NODEREF id="bed" />
      </DESCENDANTS>

    </NODE>

    <NODE id="bed2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="20" y="1.19" z="12.5" />
      <ROTATION axis="y" angle="15" />

      <DESCENDANTS>
        <NODEREF id="bed" />
      </DESCENDANTS>

    </NODE>

    <NODE id="bed">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <!-- <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="20" y="1.2" z="10" />
      <ROTATION axis="y" angle="25" /> -->

      <DESCENDANTS>
        <NODEREF id="bed_legs" />
        <NODEREF id="bed_frame" />
        <NODEREF id="bed_back" />
      </DESCENDANTS>

    </NODE>

    <!--:::::::::::::::::::::::RACKETS    :::::::-->

    <NODE id="tennis_rackets">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.4" sy="0.4" sz="0.4" />

      <DESCENDANTS>
        <NODEREF id="racket1" />
        <NODEREF id="racket2" />
      </DESCENDANTS>

    </NODE>

    <NODE id="racket1">
      <MATERIAL id="blue_tennis_racket" />
      <TEXTURE id="null" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="40" y="1.75" z="15" />
      <ROTATION axis="y" angle="50" />

      <DESCENDANTS>
        <NODEREF id="tennis_racket" />
      </DESCENDANTS>

    </NODE>

    <NODE id="racket2">
      <MATERIAL id="red_tennis_racket" />
      <TEXTURE id="null" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="35" y="3.5" z="27.5" />
      <ROTATION axis="z" angle="70" />
      <ROTATION axis="x" angle="45" />

      <DESCENDANTS>
        <NODEREF id="tennis_racket" />
      </DESCENDANTS>

    </NODE>

    <!--:::::::::::::::::::::::BIG PLANES :::::::-->

    <NODE id="sea">
      <MATERIAL id="null" />
      <TEXTURE id="water" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="0" y="0" z="20" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>

        <LEAF  type="rectangle" args="0 20 7 0" />
      </DESCENDANTS>

    </NODE>

    <NODE id="sand_ramp">
      <MATERIAL id="null" />
      <TEXTURE id="sand_to_sea" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="7" y="0" z="0" />
      <ROTATION axis="z" angle="10" />

      <ROTATION axis="x" angle="-90" />
      <ROTATION axis="z" angle="-90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 4 20 0"/>
      </DESCENDANTS>

    </NODE>

    <NODE id="sand">
      <MATERIAL id="null" />
      <TEXTURE id="sand" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="10.939" y="0.694" z="20" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 20 15 0" />
      </DESCENDANTS>

    </NODE>

    <!--::::::::::::::::::::::: SUNSHADE  :::::::-->

    <NODE id="sunshade">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="22.5" y="0.6" z="10" />

      <DESCENDANTS>
        <NODEREF id="support" />
        <NODEREF id="shade" />
      </DESCENDANTS>

    </NODE>

    <NODE id="support">
      <MATERIAL id="null" />
      <TEXTURE id="trunk" />

      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF id="cylinder_support" type="cylinder" args="4.5 0.1 0.1 20 20 0 0" />
      </DESCENDANTS>

    </NODE>

    <NODE id="shade">
      <MATERIAL id="null" />
      <TEXTURE id="bed_orange_sheet_shade" />

      <TRANSLATION x="0" y="4.55" z="0" />
      <ROTATION axis="x" angle="90" />

      <DESCENDANTS>
        <NODEREF id="shade_down" />
        <NODEREF id="shade_up" />
      </DESCENDANTS>

    </NODE>

    <NODE id="shade_up">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <LEAF id="cylinder_shade" type="cylinder" args="1 0 3 80 40 0 0" />
      </DESCENDANTS>

    </NODE>

    <NODE id="shade_down">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0" />
      <ROTATION axis="y" angle="180" />

      <DESCENDANTS>
        <LEAF id="cylinder_shade" type="cylinder" args="-1 0 3 20 40 0 0" />
      </DESCENDANTS>

    </NODE>

    <!--::::::::::::::::::::::: TABLE     :::::::-->
    <NODE id="table">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.8" sy="0.8" sz="0.8" />
      <TRANSLATION x="23" y="1.4" z="12.5" />
      <ROTATION axis="y" angle="30" />

      <DESCENDANTS>
        <NODEREF id="table_top" />
        <NODEREF id="table_legs" />
        <NODEREF id="table_cup" />
      </DESCENDANTS>

    </NODE>

    <NODE id="table_cup">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.2" sy="0.2" sz="0.2" />
      <TRANSLATION x="2" y="3.1" z="2" />

      <DESCENDANTS>
        <NODEREF id="cup" />
      </DESCENDANTS>

    </NODE>

    <NODE id="table_top">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="2" sy="0.25" sz="2" />
      <TRANSLATION x="0" y="2" z="0" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <NODE id="table_legs">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="0" y="0" z="0" />

      <DESCENDANTS>
        <NODEREF id="table_leg_1" />
        <NODEREF id="table_leg_2" />
        <NODEREF id="table_leg_3" />
        <NODEREF id="table_leg_4" />
      </DESCENDANTS>

    </NODE>

    <!-- FRONT RIGHT -->
    <NODE id="table_leg_1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.2" sy="1" sz="0.2" />
      <TRANSLATION x="4.5" y="0" z="4.5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>
    <!-- FRONT LEFT -->
    <NODE id="table_leg_2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.2" sy="1" sz="0.2" />
      <TRANSLATION x="4.5" y="0" z="-4.5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>
    <!-- BACK LEFT-->
    <NODE id="table_leg_3">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.2" sy="1" sz="0.2" />
      <TRANSLATION x="-4.5" y="0" z="-4.5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>
    <!-- BACK RIGHT-->
    <NODE id="table_leg_4">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.2" sy="1" sz="0.2" />
      <TRANSLATION x="-4.5" y="0" z="4.5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!--::::::::::::::::::::::: CUP       :::::::-->
    <NODE id="cup">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="1" sy="1" sz="1" />
      <TRANSLATION x="0" y="0" z="0" />

