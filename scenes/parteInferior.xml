<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="3" sy="3" sz="3" />
    <reference length="5" />
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
</ILLUMINATION>


<!-- Lights -->
<LIGHTS>
	<LIGHT id="light0">
        <enable value ="1" />
        <position  x="0" y="10" z="0" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

	<LIGHT id="light1">
        <enable value ="1" />
        <position x="30" y="10" z="0" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
          <enable value ="1" />
          <position x="10" y="0" z="10" w="1" />
          <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
          <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
          <specular r="1.0" g="1.0" b="1.0" a="1.0" />
      </LIGHT>

</LIGHTS>

<!-- Textures -->
<TEXTURES>
	<!-- Tree trunk -->
  <TEXTURE id="steel">
      <file path="images/leaves.jpg" />
      <amplif_factor s="2" t="2" />
  </TEXTURE>
  <TEXTURE id="wood">
      <file path="images/trunk-image.jpg" />
      <amplif_factor s="1" t="1" />
  </TEXTURE>
</TEXTURES>


<MATERIALS>
    <!-- Light bulb material -->
  <!--  <MATERIAL id="lightMaterial">
        <shininess value="100" />
        <specular r="0.5" g="0.5" b="0.5" a="1" />
        <diffuse r="0.5" g="0.5" b="0.5" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL> -->

    <MATERIAL id="defaultMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />
        <ambient r="0.4" g="0.4" b="0.4" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="steelMaterial">
      <shininess value="1" />
      <specular r="0.9" g="0.9" b="0.9" a="1" />
      <diffuse r="0.3" g="0.3" b="0.3" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

</MATERIALS>


<!-- The leaves -->
<LEAVES>
</LEAVES>


<!-- The nodes -->
<NODES>

    <ROOT id="root" />

    <!-- The root node -->
    <NODE id="root">
        <MATERIAL id="defaultMaterial" />
        <TEXTURE id="steel" />

        <DESCENDANTS>
          <!--  <NODEREF id="hip"/> -->
            <NODEREF id="nurbsTest"/>
        </DESCENDANTS>
    </NODE>

    <NODE id="nurbsTest">
        <MATERIAL id="null" />
        <TEXTURE id="null" />


        <DESCENDANTS>
             <LEAF type="patch" args="2 2">
               <CPLINE>
                <CPOINT xx="1" yy="0" zz="-1" ww="1"/>
                <CPOINT xx="2" yy="-1" zz="0" ww="3"/>
                <CPOINT xx="1" yy="0" zz="1" ww="1"/>
               </CPLINE>
               <CPLINE>
                <CPOINT xx="0" yy="0" zz="-1" ww="1"/>
                <CPOINT xx="0" yy="5" zz="0" ww="3"/>
                <CPOINT xx="0" yy="0" zz="1" ww="1"/>
               </CPLINE>
               <CPLINE>
                <CPOINT xx="-1" yy="0" zz="-1" ww="1"/>
                <CPOINT xx="-2" yy="-1" zz="0" ww="3"/>
                <CPOINT xx="-1" yy="0" zz="1" ww="1"/>
               </CPLINE>
             </LEAF>
        </DESCENDANTS>
    </NODE>

    <NODE id="hip">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
             <NODEREF id="hipBox" />
             <NODEREF id="hipPrism" />
             <NODEREF id="legLeft" />
             <NODEREF id="legRight" />
        </DESCENDANTS>
    </NODE>
    <NODE id="hipPrism">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

          <SCALE sx="1.5" sy="1" sz="1"/>

        <DESCENDANTS>
             <LEAF type="triangle" args="0.5 -0.1 0.2 -0.5 -0.1 0.2 -0.2 -0.4 0.2"/>
             <LEAF type="triangle" args="-0.2 -0.4 0.2 0.2 -0.4 0.2 0.5 -0.1 0.2"/>
             <LEAF type="triangle" args="0.5 -0.1 -0.2 -0.2 -0.4 -0.2 -0.5 -0.1 -0.2 "/>
             <LEAF type="triangle" args="-0.2 -0.4 -0.2 0.5 -0.1 -0.2 0.2 -0.4 -0.2 "/>
             <LEAF type="triangle" args="0.5 -0.1 0.2 0.2 -0.4 0.2 0.2 -0.4 -0.2"/>
             <LEAF type="triangle" args="0.5 -0.1 0.2 0.2 -0.4 -0.2 0.5 -0.1 -0.2"/>
             <LEAF type="triangle" args="-0.5 -0.1 -0.2 -0.2 -0.4 -0.2 -0.2 -0.4 0.2"/>
             <LEAF type="triangle" args="-0.5 -0.1 -0.2 -0.2 -0.4 0.2 -0.5 -0.1 0.2"/>
             <LEAF type="triangle" args="-0.2 -0.4 0.2 -0.2 -0.4 -0.2 0.2 -0.4 0.2"/>
             <LEAF type="triangle" args="0.2 -0.4 0.2 -0.2 -0.4 -0.2 0.2 -0.4 -0.2"/>
        </DESCENDANTS>
    </NODE>
    <NODE id="hipBox">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <SCALE sx="1.5" sy="0.2" sz="1"/>

        <DESCENDANTS>
             <NODEREF id="hipFront" />
             <NODEREF id="hipBack" />
             <NODEREF id="hipLeft" />
             <NODEREF id="hipRight" />
             <NODEREF id="hipTop" />
        </DESCENDANTS>
    </NODE>
    <NODE id="hipFront">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.2"/>

        <DESCENDANTS>
             <LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5"/>
        </DESCENDANTS>
    </NODE>
    <NODE id="hipBack">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <ROTATION axis="y" angle="180"/>
        <TRANSLATION x="0" y="0" z="0.2"/>

        <DESCENDANTS>
             <LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5"/>
        </DESCENDANTS>
    </NODE>
    <NODE id="hipLeft">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <ROTATION axis="y" angle="90"/>
        <TRANSLATION x="0" y="0" z="0.5"/>

        <DESCENDANTS>
             <LEAF type="rectangle" args="-0.2 0.5 0.2 -0.5"/>
        </DESCENDANTS>
    </NODE>
    <NODE id="hipRight">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <ROTATION axis="y" angle="-90"/>
        <TRANSLATION x="0" y="0" z="0.5"/>

        <DESCENDANTS>
             <LEAF type="rectangle" args="-0.2 0.5 0.2 -0.5"/>
        </DESCENDANTS>
    </NODE>
    <NODE id="hipTop">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <ROTATION axis="x" angle="-90"/>
        <TRANSLATION x="0" y="0" z="0.5"/>

        <DESCENDANTS>
             <LEAF type="rectangle" args="-0.5 0.2 0.5 -0.2"/>
        </DESCENDANTS>
    </NODE>

    <NODE id="legRight">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0.4" y="-0.2" z="-0.015"/>
        <ROTATION axis="x" angle="0" />

        <DESCENDANTS>
             <NODEREF id="leg"/>
        </DESCENDANTS>
    </NODE>

    <NODE id="legLeft">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="-0.4" y="-0.2" z="-0.015"/>
        <ROTATION axis="x" angle="0" />

        <DESCENDANTS>
             <NODEREF id="leg"/>
        </DESCENDANTS>
    </NODE>

    <NODE id="leg">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0"/>
        <ROTATION axis="x" angle="0" />

        <DESCENDANTS>
             <NODEREF id="thigh" />
        </DESCENDANTS>
    </NODE>
    <NODE id="thigh">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="80" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="0.7 0.21 0.1 5 15 1 1"/>
        <LEAF type="cylinder" args="0.7 0.15 0.25 5 15 1 1"/>
        <NODEREF id="knee" />
        <NODEREF id="calve" />
      </DESCENDANTS>
    </NODE>
    <NODE id="knee">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0.7" />
      <SCALE sx="1" sy="1" sz="0.7"/>

      <DESCENDANTS>
        <LEAF type="sphere" args="0.25 10 10"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="calve">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0.7" />
      <SCALE sx="1" sy="1" sz="0.8"/>
      <ROTATION axis="x" angle="30" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="0.7 0.23 0.05 5 15 0 0"/>
        <LEAF type="cylinder" args="0.7 0.15 0.2 5 15 0 0"/>
        <NODEREF id="boot" />
      </DESCENDANTS>
    </NODE>
    <NODE id="boot">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0.1" z="0.9" />
      <SCALE sx="0.5" sy="0.5" sz="0.5"/>
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <NODEREF id="sole" />
        <NODEREF id="bootsideRight" />
        <NODEREF id="bootsideLeft" />
        <NODEREF id="bootTop"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="sole">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0" />
      <SCALE sx="-1" sy="1" sz="-1"/>
      <ROTATION axis="x" angle="90" />

      <DESCENDANTS>
        <LEAF type="rectangle" args="-0.5 0.5 0.5 -1"/>
        <LEAF type="triangle" args="-0.5 -1 0 0 -1.3 0 0.5 -1 0"/>
        <LEAF type="rectangle" args="-0.3 0.7 0.3 0.5"/>
        <LEAF type="triangle" args="-0.5 0.5 0 -0.3 0.5 0 -0.3 0.7 0"/>
        <LEAF type="triangle" args="-0.5 0.5 0 -0.3 0.5 0 -0.3 0.7 0"/>
        <LEAF type="triangle" args="0.5 0.5 0 0.3 0.7 0 0.3 0.5 0"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="bootsideRight">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="bootside" />
      </DESCENDANTS>
    </NODE>
  <NODE id="bootsideLeft">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="toepartLeft" />
        <NODEREF id="footsideLeft" />
        <NODEREF id="heelangleLeft" />
      </DESCENDANTS>
    </NODE>
    <NODE id="bootside">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="toepart" />
        <NODEREF id="footside" />
        <NODEREF id="heelangle" />
        <NODEREF id="heelback" />
      </DESCENDANTS>
    </NODE>
    <NODE id="toepart">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="1.3"/>
      <ROTATION axis="y" angle="31"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 0.3 0.585 0"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="toepartLeft">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="1.3"/>
      <ROTATION axis="y" angle="-31"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="-0.585 0.3 0 0"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="footside">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0.5" y="0" z="0"/>
      <ROTATION axis="y" angle="90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="-1.01 0.3 0.5 0"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="footsideLeft">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="0" z="0"/>
      <ROTATION axis="y" angle="-90"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="1.01 0 -0.5 0.3"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="heelangle">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0.5" y="0" z="-0.5"/>
      <ROTATION axis="y" angle="135"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="0 0.3 0.285 0"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="heelangleLeft">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="0" z="-0.5"/>
      <ROTATION axis="y" angle="-135"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="-0.285 0.3 0 0"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="heelback">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="-0.7"/>
      <ROTATION axis="y" angle="180"/>

      <DESCENDANTS>
        <LEAF type="rectangle" args="-0.3 0.3 0.3 0"/>
      </DESCENDANTS>
    </NODE>
    <NODE id="bootTop">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0" />
      <SCALE sx="1" sy="1" sz="1"/>
      <ROTATION axis="x" angle="0" />

      <DESCENDANTS>
        <LEAF type="triangle" args="-0.4 0.5 1 0 0.3 1.3 0.4 0.5 1"/>
        <LEAF type="triangle" args="-0.4 0.5 1 -0.5 0.3 1 0 0.3 1.3"/>
        <LEAF type="triangle" args="0 0.3 1.3 0.5 0.3 1 0.4 0.5 1"/>
        <LEAF type="triangle" args="-0.4 0.5 1 0.4 0.5 1 -0.3 0.8 0"/>
        <LEAF type="triangle" args="-0.3 0.8 0 0.4 0.5 1 0.3 0.8 0"/>
        <LEAF type="triangle" args="0.4 0.5 1 0.5 0.3 1 0.5 0.3 -0.5"/>
        <LEAF type="triangle" args="0.4 0.5 1 0.5 0.3 -0.5 0.3 0.8 0"/>
        <LEAF type="triangle" args="-0.4 0.5 1 -0.5 0.3 -0.5 -0.5 0.3 1"/>
        <LEAF type="triangle" args="-0.4 0.5 1 -0.3 0.8 0 -0.5 0.3 -0.5"/>
        <LEAF type="triangle" args="0.5 0.3 -0.5 0.3 0.3 -0.7 0.3 0.8 0"/>
        <LEAF type="triangle" args="-0.5 0.3 -0.5 -0.3 0.8 0 -0.3 0.3 -0.7"/>
        <LEAF type="triangle" args="0.3 0.3 -0.7 -0.3 0.3 -0.7 0.3 0.5 -0.42"/>
        <LEAF type="triangle" args="-0.3 0.3 -0.7 -0.3 0.5 -0.42 0.3 0.5 -0.42"/>
      </DESCENDANTS>
    </NODE>
</NODES>

</SCENE>
